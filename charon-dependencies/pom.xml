            <distributionManagement>
                <repository>
                    <id>github</id>
                    <name>GitHub OWNER Apache Maven Packages</name>
                    <url>https://maven.pkg.github.com/harvies/charon</url>
                </repository>
            </distributionManagement>
        </profile>
