                <scope>runtime</scope>
            </dependency>
            <dependency>
                <groupId>taglibs</groupId>
                <artifactId>standard</artifactId>
                <version>1.1.2</version>
                <scope>runtime</scope>
            </dependency>
            <!-- other end-->

        </dependencies>
    </dependencyManagement>

    <distributionManagement>

        <snapshotRepository>
            <id>rdc-snapshots</id>
            <url>https://repo.rdc.aliyun.com/repository/130334-snapshot-p7wPh5/</url>
        </snapshotRepository>
        <repository>
            <id>rdc-releases</id>
            <url>https://repo.rdc.aliyun.com/repository/130334-release-2FjnAX/</url>
        </repository>

    </distributionManagement>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>3.8.1</version>
                    <configuration>
                        <!--在此处配置之后就不需要再安装idea插件和配置Preferences | Build, Execution, Deployment | Compiler | Annotation Processors 了 -->
                        <annotationProcessorPaths>
                            <path>
                                <groupId>org.mapstruct</groupId>
                                <artifactId>mapstruct-processor</artifactId>
                                <version>${mapstruct.version}</version>
                            </path>
                            <!-- other annotation processors -->
                            <path>
                                <groupId>org.projectlombok</groupId>
                                <artifactId>lombok</artifactId>
                                <version>${lombok.version}</version>
                            </path>
                            <path>
                                <groupId>org.springframework.boot</groupId>
                                <artifactId>spring-boot-configuration-processor</artifactId>
                                <version>${spring-boot.version}</version>
                            </path>
                            <path>
                                <groupId>org.springframework.boot</groupId>
                                <artifactId>spring-boot-autoconfigure-processor</artifactId>
                                <version>${spring-boot.version}</version>
                            </path>
                        </annotationProcessorPaths>
                    </configuration>
                </plugin>
                <!--可将依赖一起打包 -->
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-assembly-plugin</artifactId>
                    <version>3.1.1</version>
                </plugin>
                <!-- 批量修改pom版本号-->
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>versions-maven-plugin</artifactId>
                    <version>2.7</version>
                    <configuration>
                        <generateBackupPoms>false</generateBackupPoms>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-source-plugin</artifactId>
                    <version>3.0.1</version>
                    <configuration>
                        <attach>true</attach>
                    </configuration>
                    <executions>
                        <execution>
                            <phase>compile</phase>
                            <goals>
                                <goal>jar</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-javadoc-plugin</artifactId>
                    <version>3.1.1</version>
                    <executions>
                        <execution>
                            <id>attach-javadocs</id>
                            <goals>
                                <goal>jar</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-gpg-plugin</artifactId>
                    <version>1.6</version>
                    <executions>
                        <execution>
                            <id>sign-artifacts</id>
                            <phase>verify</phase>
                            <goals>
                                <goal>sign</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>

                <plugin>
                    <groupId>org.springframework.boot</groupId>
                    <artifactId>spring-boot-maven-plugin</artifactId>
                    <version>${spring-boot.version}</version>
                    <!-- 由于没有继承spring-boot-parent(里面有下面的配置),所以需要配置重新打包-->
                    <executions>
                        <execution>
                            <id>repackage</id>
                            <configuration>
                                <!--                                <mainClass>io.github.harvies.charon.spring.boot.admin.server.SpringBootAdminApplication</mainClass>-->
                            </configuration>
                            <goals>
                                <goal>repackage</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>com.google.cloud.tools</groupId>
                    <artifactId>jib-maven-plugin</artifactId>
                    <version>2.5.2</version>
                    <configuration>
                        <allowInsecureRegistries>true</allowInsecureRegistries>
                        <from>
                            <image>registry.cn-hangzhou.aliyuncs.com/harvies/jdk:latest</image>
                        </from>
                        <to>
                            <image>registry.cn-hangzhou.aliyuncs.com/harvies/${project.artifactId}:${project.version}
                            </image>
                            <tags>
                                <tag>latest</tag>
                            </tags>
                        </to>
                        <!-- Copies files from 'src/main/custom-extra-dir' and '/home/user/jib-extras' instead of 'src/main/jib' -->
                        <extraDirectories>
                            <paths>
                                <!-- Copies from '/home/user/jib-extras' into '/extras' on the container -->
                                <path>
                                    <from>/opt/charon</from>
                                    <into>/opt/charon</into>
                                </path>
                            </paths>
                        </extraDirectories>

                        <container>
                            <!--                            <mainClass>com.xxx.Application</mainClass>-->
                            <environment>
                                <SW_AGENT_COLLECTOR_BACKEND_SERVICES>sw.dev.kikera.org:11800
                                </SW_AGENT_COLLECTOR_BACKEND_SERVICES>
                            </environment>
                            <jvmFlags>
                                <jvmFlag>${Xms}</jvmFlag>
                                <jvmFlag>${Xmx}</jvmFlag>
                                <jvmFlag>${MetaspaceSize}</jvmFlag>
                                <jvmFlag>${MaxMetaspaceSize}</jvmFlag>
                                <jvmFlag>${MaxDirectMemorySize}</jvmFlag>
                                <jvmFlag>-verbose:gc</jvmFlag>
                                <jvmFlag>-Xloggc:gc.log</jvmFlag>
                                <jvmFlag>-XX:+PrintGCDetails</jvmFlag>
                                <jvmFlag>-XX:+PrintGCDateStamps</jvmFlag>
                                <jvmFlag>-XX:+PrintHeapAtGC</jvmFlag>
                                <jvmFlag>-Djava.security.egd=file:/dev/./urandom</jvmFlag>
                                <jvmFlag>-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=0.0.0.0:5005
                                </jvmFlag>
                                <jvmFlag>-javaagent:/opt/skywalking-agent/skywalking-agent.jar</jvmFlag>
                                <jvmFlag>-Dskywalking.plugin.mongodb.trace_param=true</jvmFlag>
                                <jvmFlag>-Dplugin.mysql.trace_sql_parameters=true</jvmFlag>
                                <jvmFlag>-Dplugin.mariadb.trace_sql_parameters=true</jvmFlag>
                                <jvmFlag>-Dplugin.springmvc.collect_http_params=true</jvmFlag>
                                <jvmFlag>-Dplugin.elasticsearch.trace_dsl=true</jvmFlag>
                                <jvmFlag>-Dagent.is_open_debugging_class=true</jvmFlag>
                            </jvmFlags>
                            <ports>
                                <port>${web.port}</port>
                            </ports>
                        </container>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>com.spotify</groupId>
                    <artifactId>docker-maven-plugin</artifactId>
                    <version>1.2.2</version>
                </plugin>
            </plugins>
        </pluginManagement>
        <plugins>
            <!-- 批量修改pom版本号-->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>versions-maven-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-gpg-plugin</artifactId>
                <configuration>
                    <skip>${gpg.skip}</skip>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <Xms>-Xms256M</Xms>
